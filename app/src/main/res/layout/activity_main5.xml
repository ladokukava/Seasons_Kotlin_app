<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity5">


    <TextView
        android:id="@+id/third_page_editText"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/winter_back"
        android:fontFamily="@font/archivo_black"
        android:gravity="center"
        android:padding="73dp"
        android:text="Winter"
        android:textColor="@color/black"
        android:textSize="30dp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


    <TextView
        android:id="@+id/receive1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/spring_back"
        android:fontFamily="@font/archivo_black"
        android:gravity="center"
        android:padding="73dp"
        android:textColor="@color/black"
        android:text="Spring"
        android:textSize="30dp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/third_page_editText" />


    <TextView
        android:id="@+id/receive2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/summer_back"
        android:fontFamily="@font/archivo_black"
        android:gravity="center"
        android:padding="73dp"
        android:textColor="@color/black"
        android:textSize="30dp"
        android:text="Summer"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/receive1" />


    <TextView
        android:id="@+id/receive3"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/autumn_back"
        android:fontFamily="@font/archivo_black"
        android:gravity="center"
        android:padding="72dp"
        android:textColor="@color/black"
        android:textSize="30dp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/receive2" />

</androidx.constraintlayout.widget.ConstraintLayout>